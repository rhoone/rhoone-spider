{
    "name": "rhoone/rhoone-spider",
    "description": "Spider for rho one.",
    "type": "yii2-extension",
    "keywords": ["yii2","extension","rho","one","search","spider"],
    "license": "AGPL-3.0-or-later",
    "authors": [
        {
            "name": "rhoone",
            "email": "opensource@dev.rho.one"
        }
    ],
    "require": {
        "rhosocial/yii2-base-models": "^1.1.0",
        "rhoone/yii2-rhoone": "dev-master",
        "yiisoft/yii2-redis": "^2.0.9",
        "yiisoft/yii2-queue": "^2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "rhoone\\spider\\": "",
            "rhoone\\spider\\redis\\": "drivers/redis"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\": "tests"
        }
    },
    "repositories":[
        {
            "type": "git",
            "url": "https://github.com/rhoone/rhoone-spider"
        }
    ]
}
